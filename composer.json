{
  "name": "muraveiko/codeigniter-smarty-parser",
  "type": "library",
  "minimum-stability" : "dev",
  "description": "Smarty templating for Codeigniter as CI_Parser",
  "license": "MIT",
  "authors": [
    {
      "name": "Oleg Muraveyko",
      "email": "oleg@muraveyko.ru"
    }
  ],
  "require": {
    "smarty/smarty": "v3.1.*"
  },
  "suggest": {
